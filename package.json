    "uppercamelcase": "3.0.0",
    "url-loader": "^1.0.1",
    "vue": "^2.5.9",
    "vue-jest": "^2.1.1",
    "vue-loader": "^14.0.0",
    "vue-markdown-loader": "^2.3.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.9",
    "vuex": "3.0.1",
    "webpack": "^3.9.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.10.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 7",
    "last 2 versions",
    "not ie <= 8"
  ]
}
