    "node-sass": "^4.7.2",
    "normalize.css": "^8.0.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^2.0.0",
    "particles.js": "^2.0.0",
    "perfect-scrollbar": "^1.3.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
